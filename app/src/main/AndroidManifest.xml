<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learningapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Codex"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".WebViewSupport.Show_history_activity" />
        <activity
            android:name=".webViewes.Show_saved_pages_web_view"
            android:theme="@style/web_view_action_bar" />
        <activity android:name=".WebViewSupport.Show_saved_pages" />
        <activity android:name=".Profile.Show_profile" />
        <activity
            android:name=".webViewes.Books_web_view"
            android:theme="@style/web_view_action_bar" />
        <activity
            android:name=".webViewes.Youtube_web_view"
            android:theme="@style/web_view_action_bar" />
        <activity
            android:name=".webViewes.Website_web_view"
            android:theme="@style/web_view_action_bar" />
        <activity android:name=".Profile.Edit_profile" />
        <activity android:name=".Dashboard.Main_dashboard" />
        <activity android:name=".UserAuthentication.Signup_activity" />
        <activity android:name=".UserAuthentication.Forgot_pass_activity" />
        <activity android:name=".UserAuthentication.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>